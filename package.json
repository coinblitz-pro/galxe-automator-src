{
  "name": "galxe-automator",
  "private": true,
  "version": "1.3.0",
  "author": "Ivan Lagunovsky <ivan.lagunovsky@gmail.com>",
  "bin": "./dist/bundle.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "transpile": "tsc",
    "bundle": "esbuild dist/index.js --bundle --minify --outfile=dist/bundle.js --format=cjs --platform=node",
    "package": "pkg package.json",
    "build": "rm -rf bin && npm run transpile && npm run bundle && npm run package && ts-node utils/postbuild.ts && rm -rf dist",
    "sign": "ts-node utils/sign.ts"
  },
  "devDependencies": {
    "@types/md5": "^2.3.2",
    "@types/node": "^20.3.3",
    "@types/tar": "^6.1.5",
    "axios": "^1.4.0",
    "chalk": "^4.1.2",
    "dayjs": "^1.11.9",
    "esbuild": "^0.18.11",
    "ethers": "^5.7.2",
    "gradient-string": "^2.0.2",
    "https-proxy-agent": "^7.0.0",
    "inquirer": "^8.2.5",
    "md5": "^2.3.0",
    "moize": "^6.1.6",
    "pkg": "^5.8.1",
    "qs": "^6.11.2",
    "systeminformation": "^5.18.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "pkg": {
    "scripts": "dist/bundle.js",
    "outputPath": "bin",
    "compress": true,
    "targets": [
      "node16-linux-x64",
      "node16-linux-arm64",
      "node16-win-x64",
      "node16-win-arm64",
      "node16-mac-x64",
      "node16-mac-arm64"
    ]
  }
}
